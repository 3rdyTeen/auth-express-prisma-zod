{
  "name": "express-prisma-mysql-zod-template",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "dev": "tsx watch ./src/index.ts",
    "clean": "rm -rf ./dist",
    "build": "npm clean && tsc -p tsconfig.build.json && tsc-alias",
    "start": "node ./dist/index.js",
    "db:create": "prisma migrate dev --create-only",
    "db:generate": "prisma generate",
    "db:deploy": "prisma migrate deploy",
    "db:status": "prisma migrate status",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm lint --fix",
    "format": "prettier --write '**/*.ts{,x}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/node": "^22.1.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0"
  }
}
